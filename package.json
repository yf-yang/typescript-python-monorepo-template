{
  "name": "whatever",
  "version": "1.0.0",
  "description": "TypeScript+Python Hybrid Monorepo Template",
  "type": "module",
  "scripts": {
    "initialize": "concurrently \"pnpm install\" \"pnpm sync\"",
    "sync": "uv sync",
    "test": "uv run pytest",
    "dev:backend": "fastapi dev ./pyPackages/app/app/main.py --port 10086",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\""
  },
  "devDependencies": {
    "@jest/expect": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@rsbuild/core": "1.3.18",
    "@rslib/core": "^0.6.9",
    "@whatever/eslint-config": "workspace:^",
    "@whatever/jest-config": "workspace:^",
    "@whatever/ts-config": "workspace:^",
    "concurrently": "^9.1.2",
    "cspell": "^9.0.1",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "packageManager": "pnpm@10.10.0"
}
